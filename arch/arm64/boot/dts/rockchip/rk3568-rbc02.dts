&scmi {
	status = "disabled";
};


&cpu0 {
	cpu-supply = <&vdd_cpu>;
};

&cpu1 {
	cpu-supply = <&vdd_cpu>;
};

&cpu2 {
	cpu-supply = <&vdd_cpu>;
};

&cpu3 {
	cpu-supply = <&vdd_cpu>;
};

&uart0 {
	status = "okay";
	pinctrl-0 = <&uart0_xfer &com1_de>;
	rs485-term-gpios = <&gpio0 RK_PC3 GPIO_ACTIVE_HIGH>;
	rs485-rx-during-tx;
	linux,rs485-enabled-at-boot-time;
};

&uart2 {
	status = "okay";
};

&uart3 {
	status = "okay";
	pinctrl-0 = <&uart3m1_xfer &com2_de>;
	rs485-term-gpios = <&gpio3 RK_PA4 GPIO_ACTIVE_HIGH>;
	rs485-rx-during-tx;
	linux,rs485-enabled-at-boot-time;
};

&uart4 {
	status = "okay";
	pinctrl-0 = <&uart4m1_xfer &com3_de>;
	rs485-term-gpios = <&gpio3 RK_PB6 GPIO_ACTIVE_HIGH>;
	rs485-rx-during-tx;
	linux,rs485-enabled-at-boot-time;
};

&uart5 {
	status = "okay";
	pinctrl-0 = <&uart5m1_xfer &com4_de>;
	rs485-term-gpios = <&gpio4 RK_PD2 GPIO_ACTIVE_HIGH>;
	rs485-rx-during-tx;
	linux,rs485-enabled-at-boot-time;
};

&uart8 {
	status = "okay";
	pinctrl-0 = <&uart8m1_xfer &com5_de>;
	rs485-term-gpios = <&gpio3 RK_PA3 GPIO_ACTIVE_HIGH>;
	rs485-rx-during-tx;
	linux,rs485-enabled-at-boot-time;
};

&uart9 {
	status = "okay";
	pinctrl-0 = <&uart9m1_xfer &com6_de>;
	rs485-term-gpios = <&gpio4 RK_PC2 GPIO_ACTIVE_HIGH>;
	rs485-rx-during-tx;
	linux,rs485-enabled-at-boot-time;
