/dts-v1/;
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/leds/common.h>
#include <dt-bindings/pinctrl/rockchip.h>
#include <dt-bindings/soc/rockchip,vop2.h>
#include "rk3568.dtsi"

/ {
	model = "Xbrother RBC02-PRO Board";
	compatible = "xbrother,rbc02-pro", "rockchip,rk3568";

	aliases {
		/delete-property/ serial0;
		/delete-property/ serial1;
		/delete-property/ serial2;
		/delete-property/ serial3;
		/delete-property/ serial4;
		/delete-property/ serial5;
		/delete-property/ serial6;
		/delete-property/ serial7;
		/delete-property/ serial8;
		/delete-property/ serial9;
		/delete-property/ i2c1;
		/delete-property/ i2c2;
		/delete-property/ i2c3;
		/delete-property/ i2c4;
		/delete-property/ i2c5;
		/delete-property/ spi0;
		/delete-property/ spi1;
		/delete-property/ spi2;
		/delete-property/ spi3;
		serial0 = &uart2;
		serial1 = &uart0;
		serial2 = &uart3;
		serial3 = &uart4;
		serial4 = &uart5;
		serial5 = &uart8;
		serial6 = &uart9;
		ethernet0 = &gmac1;
		ethernet1 = &gmac0;
		mmc0 = &sdhci;
		mmc1 = &sdmmc0;
		usb1 = &usb_host0_ehci;
		usb2 = &usb_host1_ehci;
		usb3 = &usb_host0_ohci;
		usb4 = &usb_host1_ohci;
		usb7 = &usb_host0_xhci;
		usb5 = &usb_host1_xhci;
		i2c1 = &gpioi2c1;
		i2c2 = &gpioi2c2;
		i2c5 = &gpioi2c5;
	};

	chosen: chosen {
		stdout-path = "serial0:115200n8";
	};

	dc_12v: dc-12v {
		compatible = "regulator-fixed";
		regulator-name = "dc_12v";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <12000000>;
		regulator-max-microvolt = <12000000>;
	};

	leds {
		compatible = "gpio-leds";
		heartbeat: heartbeat {
			label = "heartbeat";
			gpios = <&gpio4 RK_PC4 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "heartbeat";
		};
	};

	xbrother_gpio {
		compatible = "xbrother,gpio";
		#size-cells = <0>;
		sys_name = "xbrother";
		di08 {
			label = "di08";
			gpios = <&tca6424 0 GPIO_ACTIVE_LOW>;
			direction = "input";
		};
		di07 {
			label = "di07";
			gpios = <&tca6424 1 GPIO_ACTIVE_LOW>;
			direction = "input";
		};
		di06 {
			label = "di06";
			gpios = <&tca6424 2 GPIO_ACTIVE_LOW>;
			direction = "input";
		};
		di05 {
			label = "di05";
			gpios = <&tca6424 3 GPIO_ACTIVE_LOW>;
			direction = "input";
		};
		di04 {
			label = "di04";
			gpios = <&tca6424 4 GPIO_ACTIVE_LOW>;
			direction = "input";
		};
		di03 {
			label = "di03";
			gpios = <&tca6424 5 GPIO_ACTIVE_LOW>;
			direction = "input";
		};
		di02 {
			label = "di02";
			gpios = <&tca6424 6 GPIO_ACTIVE_LOW>;
			direction = "input";
		};
		di01 {
			label = "di01";
			gpios = <&tca6424 7 GPIO_ACTIVE_LOW>;
			direction = "input";
		};
		do01 {
			label = "do01";
			gpios = <&tca6424 8 GPIO_ACTIVE_HIGH>;
			direction = "low";
		};
		do02 {
			label= "do02";
			gpios = <&tca6424 9 GPIO_ACTIVE_HIGH>;
			direction = "low";
		};
		e2wp {
			label = "e2wp";
			gpios = <&gpio1 0 GPIO_ACTIVE_HIGH>;
			direction = "high";
		};
		12va-ctl {
			label = "12va-ctl";
			gpios = <&gpio1 8 GPIO_ACTIVE_LOW>;
			direction = "low";
		};
		12vb-ctl {
			label = "12vb-ctl";
			gpios = <&gpio1 10 GPIO_ACTIVE_LOW>;
			direction = "low";
		};
		12va-det {
			label = "12va-det";
			gpios = <&gpio3 5 GPIO_ACTIVE_HIGH>;
			direction = "input";
		};
		12vb-det {
			label = "12vb-det";
			gpios = <&gpio3 6 GPIO_ACTIVE_HIGH>;
			direction = "input";
		};
		12v-det {
			label = "12v-det";
			gpios = <&gpio3 7 GPIO_ACTIVE_HIGH>;
			direction = "input";
		};
		fan {
			label = "fan";
			gpios = <&gpio0 29 GPIO_ACTIVE_HIGH>;
			direction = "low";
		};
	};

	gpioi2c1: i2c-1 {
		status = "okay";
		compatible = "i2c-gpio";
		sda-gpios = <&gpio0 RK_PB3 GPIO_ACTIVE_HIGH>;
		scl-gpios = <&gpio0 RK_PB4 GPIO_ACTIVE_HIGH >;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c1_xfer>;
		i2c-gpio,delay-us = <5>;	/* ~100 kHz */
		#address-cells = <1>;
		#size-cells = <0>;

		eeprom@50 {
			compatible = "atmel,24c04";
			reg = <0x50>;
			pagesize = <16>;
		};
	};

	gpioi2c2: i2c-2 {
		status = "okay";
		compatible = "i2c-gpio";
		sda-gpios = <&gpio0 RK_PB6 GPIO_ACTIVE_HIGH>;
		scl-gpios = <&gpio0 RK_PB5 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c2m0_xfer>;
		i2c-gpio,delay-us = <5>;	/* ~100 kHz */
		#address-cells = <1>;
		#size-cells = <0>;
	};

	gpioi2c5: i2c-5 {
		status = "okay";
		compatible = "i2c-gpio";
		sda-gpios = <&gpio3 RK_PB4 GPIO_ACTIVE_HIGH>;
		scl-gpios = <&gpio3 RK_PB3 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "default";
		pinctrl-0 = <&i2c5m0_xfer>;
		i2c-gpio,delay-us = <5>;	/* ~100 kHz */
		#address-cells = <1>;
		#size-cells = <0>;
		hym8563: hym8563@51 {
			compatible = "haoyu,hym8563";
			reg = <0x51>;
		};
	};

	vcc3v3_sys: vcc3v3-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc3v3_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
		vin-supply = <&dc_12v>;
	};

	vcc5v0_sys: vcc5v0-sys {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_sys";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		vin-supply = <&dc_12v>;
	};

	vcc5v0_usb: vcc5v0-usb {
		compatible = "regulator-fixed";
		regulator-name = "vcc5v0_usb";
		regulator-always-on;
		regulator-boot-on;
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		vin-supply = <&dc_12v>;
	};

	
};


